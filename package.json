{
  "name": "openapi-cod-gen",
  "version": "0.0.1",
  "description": "API code-gen",
  "main": "dist/main.js",
  "bin": {
    "openapi": "bin/index.js"
  },
  "files": [
    "bin/index.js",
    "dist/main.js"
  ],
  "scripts": {
    "clear": "rimraf ./dist ./node_modules/.cache",
    "dev": "webpack serve",
    "example": "node index.js -i ./example.yaml -o ./example",
    "build": "vite build",
    "example:build": "node ./bin/index.js -i ./example.yaml -o ./example-build"
  },
  "author": "dontmeway",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "handlebars-loader": "^1.7.2",
    "vite": "^3.1.8",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "resolutions": {
    "resolve": "1.15.1"
  }
}
